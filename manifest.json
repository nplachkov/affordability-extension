{
  "manifest_version": 3,
  "name": "Affordability Calculator",
  "version": "6.9",
  "description": "An affordability calculator made from the Flex Team, for the Flex Team.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "default_title": "NMW Calculator"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "storage"
  ]
}